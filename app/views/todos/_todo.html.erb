<td><%= todo.summary %></td>
<td><%= todo.priority %></td>
<td><%= todo.due_at %></td>
<td><%= todo.complete %></td>
<td>
  <%= form_with(model: todo, local: false) do |form| %>
    <div class="field">
      <%= form.label :complete %>
      <%= form.check_box :complete, class: 'checkbox' %>
    </div>
  <% end %>
</td>
<td><%= link_to 'Show', todo %></td>
<td><%= link_to 'Edit', edit_todo_path(todo) %></td>
